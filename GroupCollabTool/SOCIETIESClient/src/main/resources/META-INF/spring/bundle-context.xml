<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

  <!-- regular spring configuration file defining the beans for this
       bundle. We've kept the osgi definitions in a separate 
       configuration file so that this file can easily be used
       for integration testing outside of an OSGi environment -->
	
	

 
  <bean id="GroupCollabToolClient" 
  		class="org.societies.GroupCollabtoolClient.GroupCollabToolClient" 
  		init-method="initService" >
  		<property name="uam" ref="uam"/>
  		<property name="commsMgr" ref="commsMgr"/>
  		<property name="servics" ref="servics"/>
   	 
  </bean>
  
</beans>
